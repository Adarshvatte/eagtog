{
    "builds": [{
        "src": "eatog/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "functions": {
        "api/wsgi.py": {
            "memory": 1700,  // Adjust memory as needed
            "maxDuration": 30  // Increase max duration if your function needs more time
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "eatog/wsgi.py"
        }
    ]
}

