{
    "builds": [{
        "src": "eatog/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.9",
            "maxDuration": 30,  // Increase max duration if your function needs more time
            "memory": 1700      // Adjust memory as needed
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "eatog/wsgi.py"
        }
    ]
}
